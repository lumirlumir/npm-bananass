{
  "name": "bananass-utils-vitepress",
  "version": "0.1.1",
  "type": "module",
  "description": "VitePress Utilities for Bananass Framework.üçå",
  "exports": {
    "./head": {
      "types": "./build/head/index.d.ts",
      "default": "./src/head/index.js"
    },
    "./i18n": {
      "types": "./build/i18n/index.d.ts",
      "default": "./src/i18n/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./build/*"
      ],
      "head": [
        "./build/head/index.d.ts"
      ],
      "i18n": [
        "./build/i18n/index.d.ts"
      ]
    }
  },
  "files": [
    "src",
    "build",
    "LICENSE.md",
    "README.md",
    "!src/**/*.test.js",
    "!**/fixtures/**"
  ],
  "keywords": [
    "bananass",
    "baekjoon",
    "framework",
    "javascript",
    "typescript",
    "utils",
    "vitepress"
  ],
  "author": "Î£®Î∞ÄLuMir <rpfos@naver.com> (https://github.com/lumirlumir)",
  "license": "MIT",
  "homepage": "https://bananass.lumir.page",
  "repository": {
    "type": "github",
    "url": "git+https://github.com/lumirlumir/npm-bananass.git",
    "directory": "packages/bananass-utils-vitepress"
  },
  "bugs": {
    "url": "https://github.com/lumirlumir/npm-bananass/issues"
  },
  "publishConfig": {
    "provenance": true
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npx tsc && shx cp ../../LICENSE.md ../../README.md .",
    "test": "node --test"
  }
}
