{
  "name": "bananass",
  "version": "0.1.0-canary.2",
  "type": "module",
  "description": "Baekjoon Framework for JavaScript.üçå",
  "exports": {
    ".": {
      "types": "./build/core/types.d.ts"
    },
    "./commands": {
      "types": "./build/commands/index.d.ts",
      "default": "./src/commands/index.js"
    },
    "./core/structs": {
      "types": "./build/core/structs/index.d.ts",
      "default": "./src/core/structs/index.js"
    },
    "./core/constants": {
      "types": "./build/core/constants.d.ts",
      "default": "./src/core/constants.js"
    },
    "./core/types": {
      "types": "./build/core/types.d.ts",
      "default": "./src/core/types.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./build/*"
      ],
      ".": [
        "./build/core/types.d.ts"
      ],
      "./commands": [
        "./build/commands/index.d.ts"
      ],
      "./core/structs": [
        "./build/core/structs/index.d.ts"
      ],
      "./core/constants": [
        "./build/core/constants.d.ts"
      ],
      "./core/types": [
        "./build/core/types.d.ts"
      ]
    }
  },
  "bin": {
    "b": "src/cli.js",
    "bananass": "src/cli.js"
  },
  "files": [
    "src",
    "build",
    "LICENSE.md",
    "README.md",
    "!src/**/*.test.js",
    "!**/fixtures/**"
  ],
  "keywords": [
    "bananass",
    "bananas",
    "banana",
    "baekjoon",
    "framework",
    "javascript"
  ],
  "author": "Î£®Î∞ÄLuMir <rpfos@naver.com> (https://github.com/lumirlumir)",
  "license": "MIT",
  "homepage": "https://bananass.lumir.page",
  "repository": {
    "type": "github",
    "url": "git+https://github.com/lumirlumir/npm-bananass.git",
    "directory": "packages/bananass"
  },
  "bugs": {
    "url": "https://github.com/lumirlumir/npm-bananass/issues"
  },
  "engines": {
    "node": "^20.18.0 || >= 22.3.0"
  },
  "publishConfig": {
    "provenance": true
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npx tsc && shx cp ../../LICENSE.md ../../README.md .",
    "test": "node --experimental-test-module-mocks --test",
    "dev": "node src/cli.js"
  },
  "dependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "babel-loader": "^10.0.0",
    "bananass-utils-console": "^0.1.0-canary.2",
    "c12": "^3.0.3",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "enhanced-resolve": "^5.18.1",
    "envinfo": "^7.14.0",
    "esbuild-loader": "^4.3.0",
    "jiti": "^2.4.2",
    "open": "^10.1.1",
    "superstruct": "^2.0.2",
    "webpack": "^5.99.6"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5"
  }
}
